# CM resource apply
FN_AUTO_CM_RESOURCE(){
    STEP_FUNCTION="FN_AUTO_CM_RESOURCE"
    echo
    echo "############################################################"
    echo "# tbis Progress> tbcm resource "    
    echo "############################################################"
    echo
    $TBIS_SHELL tbis.cfg CM_RESOURCE
    echo
    FN_AUTO_PROGRESS_CHECK
    FN_AUTO_PROGRESS_PROFILE
    . $TIBERO_PROFILE_CHECK

    if [ "$YESNO" == "YES" ]
    then
        if [ "$INSTALL_USER_NAME" == "root" ]
        then
